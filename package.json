{
  "name": "brisky-events",
  "version": "1.2.1",
  "description": "Adds event delegation and custom events to Brisky",
  "main": "lib/index.js",
  "scripts": {
    "start": "watchify-server test/index.js",
    "test": "node test | tap-difflet && standard",
    "watch": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-events.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-events/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-events#readme",
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "browser": {
    "./lib/listener": "./lib/listener/browser"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "vigour-base": "^3.3.6",
    "vigour-stamp": "^1.2.0",
    "vigour-ua": "^1.0.3",
    "vigour-util": "^3.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0"
  },
  "contributors": [
    "Jim de Beer <jim@vigour.io>",
    "Youri Daamen <youri@vigour.io>"
  ],
  "devDependencies": {
    "brisky-core": "^1.1.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "nodemon": "^1.9.1",
    "pre-commit": "^1.1.3",
    "standard": "^7.0.1",
    "tap-difflet": "0.4.0",
    "tape": "^4.4.0",
    "vigour-doc": "^1.1.5",
    "watchify-server": "^1.0.2"
  }
}
